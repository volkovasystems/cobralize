{
  "name": "cobralize",
  "version": "0.5.0",
  "description": "Transform any human readable alphanumeric string divided by certain characters into underscore separated uppercase form.",
  "main": "cobralize.js",
  "support": "cobralize.support.js",
  "deploy": "cobralize.deploy.js",
  "authors": [
    "Richeve S. Bebedor <richeve.bebedor@gmail.com>",
    "John Lenon Maghanoy <johnlenonmaghanoy@gmail.com>"
  ],
  "license": "MIT",
  "keywords": [
    "snake-case",
    "cobralize",
    "uppercase"
  ],
  "homepage": "https://github.com/volkovasystems/cobralize",
  "moduleType": [
    "es6",
    "globals",
    "node"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "disdo": "^0.5.0",
    "falzy": "^0.8.0",
    "protype": "^0.9.0"
  }
}
